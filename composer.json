{
    "name": "md0/regenerator",
    "description": "Laravel ReGenerator",
    "license": "AGPL-3.0",
    "authors": [
        {
            "name": "Matei Dobrescu",
            "email": "contact@md0.ro"
        }
    ],
    "homepage": "https://github.com/md0-code/regenerator",
    "keywords": [
        "Laravel", "ReGenerator", "report generator"
    ],
    "require": {
        "laravel/framework": "^10.0|^9.0|^8.0|^7.0",
        "doctrine/dbal": "^2.5|^3.0",
        "dompdf/dompdf": "^1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "MD0\\ReGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MD0\\ReGenerator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MD0\\ReGenerator\\ReGeneratorServiceProvider"
            ]
        }
    },
        "suggest": {
        "md0/backpack-regenerator": "Backpack-based GUI interface for ReGenerator"
    }
}
